    "react-loadable": "^5.4.0"
  },
  "devDependencies": {
    "@dx-groups/athena": "^2.1.0",
    "babel-plugin-import": "^1.8.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.9.1"
  },
  "pre-commit": [
    "lint"
  ]
}
